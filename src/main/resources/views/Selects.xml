<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <selection name="product.display.select">
    <option value="select">Select</option>
    <option value="checkbox">Checkbox</option>
  </selection>

  <selection name="price.after.login.select">
    <option value="fromDB">From DB</option>
    <option value="fromWS">From WS</option>
  </selection>

  <selection name="allow.registration.select">
    <option value="yesForAll">Yes for all</option>
    <option value="yesForAOS">Yes if registered on AOS</option>
    <option value="no">No</option>
  </selection>

  <selection name="app.install.select">
    <option value="yes">Yes</option>
    <option value="no">No</option>
  </selection>

  <selection name="public.eshop.select">
    <option value="yes">Yes</option>
    <option value="no">No</option>
  </selection>

  <selection name="portal.role.select">
    <option value="restricted">Restricted</option>
    <option value="total">Total</option>
  </selection>

  <selection name="payment.config.type.select">
    <option value="paypal">Paypal</option>
    <option value="stripe">Stripe</option>
  </selection>

  <selection name="main.price.select">
    <option value="at">AT</option>
    <option value="wt">WT</option>
  </selection>

  <selection name="display.two.prices.select">
    <option value="yes">Yes</option>
    <option value="no">No</option>
  </selection>

  <selection name="navigation.select">
    <option value="leftSide">Left side</option>
    <option value="topSide">Top side</option>
  </selection>

  <selection name="notification.select">
    <option value="all">All new post and new comments</option>
    <option value="allOnMyPost">All new post and new comments on my post</option>
    <option value="newCommentsOnMyPost">Only new comments on my post</option>
    <option value="none">No notification</option>
  </selection>

  <selection name="reaction.image.select">
    <option value="like" icon="img/forumReaction/Like-icon.png">Like</option>
    <option value="dislike" icon="img/forumReaction/Dislike-icon.png">Dislike</option>
    <option value="love" icon="img/forumReaction/Love-icon.png">Love</option>
    <option value="sad" icon="img/forumReaction/Sad-icon.png">Sad</option>
    <option value="angry" icon="img/forumReaction/Angry-icon.png">Angry</option>
    <option value="funny" icon="img/forumReaction/Funny-icon.png">Funny</option>
  </selection>

  <selection name="access.type.select">
    <option value="allPartners">Allow all partners that have access to the app</option>
    <option value="selectedPartners">Only selected partners</option>
    <option value="selectedCategories">Only selected partner categories</option>
  </selection>

  <selection name="subscribe.type.select">
    <option value="always">Always subscribe</option>
    <option value="groupSubscribe">Only if user subscribe to the group</option>
  </selection>

  <selection name="can.pay.invoice.select">
    <option value="no">No</option>
    <option value="total">Total</option>
    <option value="partial">Partial</option>
  </selection>

  <selection name="ticket.status.change.method.select">
    <option value="fromDB">From DB</option>
    <option value="fromWS">From WS</option>
  </selection>

  <selection name="social.media.select">
    <option value="linkedin">LinkedIn</option>
    <option value="twitter">Twitter</option>
    <option value="instagram">Instagram</option>
    <option value="whatsapp">Wjhatsapp</option>
  </selection>

  <selection name="project.chat.visibility" id="portal.project.chat.visibility">
    <option value="3" order="3">Customer chat</option>
  </selection>

  <selection name="dms.file.permission.select">
    <option value="write">Can write</option>
    <option value="upload">Can upload</option>
  </selection>

  <selection name="partner.created.from.select">
    <option value="backend">Back-end</option>
    <option value="portal">Portal</option>
  </selection>

  <selection name="no.more.stock.select">
    <option value="1">Allow to buy and don't show message "out of stock"</option>
    <option value="2">Allow to buy and show message to alert buyer</option>
    <option value="3">Don't allow to buy and hide product</option>
    <option value="4">Don't allow to buy and show "out of stock"</option>
  </selection>

  <selection name="portal.icon.select">
    <option value="OutlinePerson">Outline Person</option>
  </selection>

  <selection name="portal.event.status.select">
    <option value="0">Draft</option>
    <option value="1">Published</option>
  </selection>

</object-views>
