<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="portal-forum-notification-config-grid" title="Notification configs"
    model="com.axelor.apps.portal.db.PortalForumNotificationConfig" editable="true">
    <field name="activateNotification"/>
    <field name="forumGroup"/>
  </grid>

  <form name="portal-forum-notification-config-form" title="Notification config"
    model="com.axelor.apps.portal.db.PortalForumNotificationConfig">
    <panel name="mainPanel">
      <field name="activateNotification"/>
      <field name="forumGroup"/>
    </panel>
  </form>

</object-views>