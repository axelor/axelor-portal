<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="portal-contact-permission-grid" title="Contact permissions"
    model="com.axelor.apps.portal.db.PortalContactPermission">
    <field name="name"/>
    <field name="isDefault"/>
  </grid>

  <form name="portal-contact-permission-form" title="Contact permission"
    model="com.axelor.apps.portal.db.PortalContactPermission">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="isDefault"/>
      <field name="portalContactWorkspaceList" colSpan="12"/>
    </panel>
  </form>

</object-views>