<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="portal-partner-permission-grid" title="Partner permissions"
    model="com.axelor.apps.portal.db.PortalPartnerPermission">
    <field name="name"/>
    <field name="isDefault"/>
  </grid>

  <form name="portal-partner-permission-form" title="Partner permission"
    model="com.axelor.apps.portal.db.PortalPartnerPermission">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="isDefault"/>
      <field name="partnerPortalWorkspaceList" colSpan="12"/>
    </panel>
  </form>

</object-views>