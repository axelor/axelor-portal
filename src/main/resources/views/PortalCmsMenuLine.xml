<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid model="com.axelor.apps.portal.db.PortalCmsMenuLine" title="CMS menu lines"
    name="portal-portal-cms-menu-line-grid">
    <field name="title"/>
    <field name="code"/>
    <field name="page"/>
  </grid>

  <form model="com.axelor.apps.portal.db.PortalCmsMenuLine" title="CMS menu line"
    name="portal-portal-cms-menu-line-form" width="large">
    <panel name="mainPanel" colSpan="12">
      <field name="title" required="true"/>
      <field name="code"/>
      <field name="page"/>
      <field name="subMenuList" colSpan="12"/>
    </panel>
  </form>

</object-views>