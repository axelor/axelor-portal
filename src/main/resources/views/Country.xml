<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <form model="com.axelor.apps.base.db.Country" title="Country" name="country-form"
    id="portal-country-form" extension="true">
    <extend target="//panel[@name='mainPanel']">
      <insert position="inside">
        <field name="fiscalPosition" form-view="fiscal-position-form"
          grid-view="fiscal-position-grid"/>
        <field name="partnerPriceList" form-view="partner-price-list-form"
          grid-view="partner-price-list-grid"/>
      </insert>
    </extend>
  </form>

</object-views>