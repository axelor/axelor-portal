  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="portal-permission-grid" title="Permissions"
    model="com.axelor.apps.portal.db.PortalPermission">
    <field name="portalWorkspace"/>
    <field name="portalApp"/>
    <field name="roleSelect"/>
  </grid>

  <form name="portal-permission-form" title="Permission"
    model="com.axelor.apps.portal.db.PortalPermission">
    <panel name="mainPanel">
      <field name="portalWorkspace"/>
      <field name="portalApp"/>
      <field name="roleSelect"/>
    </panel>
  </form>

</object-views>