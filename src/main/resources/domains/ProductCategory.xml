<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="ProductCategory">

    <many-to-many ref="com.axelor.apps.base.db.ProductVariantConfig"
      name="portalVariantConfigSet" title="Portal variants"/>
    <many-to-one ref="com.axelor.meta.db.MetaFile" name="portalImage"
      title="Portal image"/>
    <many-to-one name="portalWorkspace" ref="com.axelor.apps.portal.db.PortalWorkspace"
      title="Workspace"/>
    <string name="subtitle" title="Subtitle"/>
    <boolean name="filterOnBrand" title="Filter On Brands"/>

  </entity>

</domain-models>
