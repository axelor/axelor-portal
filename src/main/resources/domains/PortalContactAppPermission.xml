<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="portal" package="com.axelor.apps.portal.db"/>

  <entity name="PortalContactAppPermission">

    <many-to-one name="app" ref="com.axelor.apps.portal.db.PortalApp" title="App"/>
    <many-to-one name="contactWorkspaceConfig"
      ref="com.axelor.apps.portal.db.PortalContactWorkspaceConfig"/>
    <string name="roleSelect" selection="portal.role.select" title="Role"/>

    <extra-code><![CDATA[
      public static String PORTAL_CONTACT_APP_PERMISSION_ROLE_RESTRICTED = "restricted";
      public static String PORTAL_CONTACT_APP_PERMISSION_ROLE_TOTAL = "total";
    ]]></extra-code>
  </entity>

</domain-models>
