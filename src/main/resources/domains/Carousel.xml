<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module package="com.axelor.apps.portal.db" name="portal"/>

  <entity name="Carousel">
    <many-to-one ref="com.axelor.meta.db.MetaFile" name="image"/>
    <string name="title" title="Title"/>
    <string name="subTitle" title="Sub title"/>
    <string name="buttonLabel"/>
    <string name="href"/>
    <many-to-one ref="com.axelor.apps.portal.db.PortalAppConfig" name="portalAppConfig"/>
  </entity>

</domain-models>