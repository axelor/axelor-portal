<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module package="com.axelor.apps.portal.db" name="portal"/>

  <entity name="PortalCmsMainWebsite">
    <string name="name" title="Name"/>
    <many-to-one ref="com.axelor.apps.portal.db.PortalCmsSite" name="defaultWebsite"/>
    <one-to-many ref="com.axelor.apps.portal.db.PortalCmsSiteLanguage"
      name="languageList" title="Languages"/>
    <many-to-many ref="com.axelor.apps.portal.db.PortalWorkspace" name="workspaceSet"
      title="Workspaces"/>
  </entity>

</domain-models>