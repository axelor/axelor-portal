<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="Partner">

    <string name="portalPassword" title="Password" encrypted="true"/>
    <many-to-one name="websiteConfig" ref="com.axelor.apps.portal.db.WebsiteConfig"
      title="Website configuration"/>
    <one-to-many name="partnerPortalWorkspaceList"
      ref="com.axelor.apps.portal.db.PartnerPortalWorkspace" title="Workspaces" mappedBy="partner"/>

  </entity>

</domain-models>
