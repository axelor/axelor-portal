<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="sale" package="com.axelor.apps.sale.db"/>

  <entity name="SaleOrder" implements="com.axelor.apps.base.interfaces.PricedOrder">

    <many-to-one name="portalWorkspace" ref="com.axelor.apps.portal.db.PortalWorkspace"
      title="Workspace"/>
    <many-to-one ref="com.axelor.meta.db.MetaFile" name="finalizedReport"
      title="Finalized report" readonly="true" copy="false"/>
    <many-to-one ref="com.axelor.meta.db.MetaFile" name="confimedReport"
      title="Confirmed report" readonly="true" copy="false"/>

  </entity>

</domain-models>