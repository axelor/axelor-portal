<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="portal" package="com.axelor.apps.portal.db"/>

  <entity name="PortalContactWorkspaceConfig">

    <string name="name"/>
    <boolean name="isAdmin" title="Admin"/>
    <many-to-one name="portalWorkspace" ref="com.axelor.apps.portal.db.PortalWorkspace"
      title="Workspace"/>
    <one-to-many name="contactAppPermissionList" title="App permissions"
      ref="com.axelor.apps.portal.db.PortalContactAppPermission" mappedBy="contactWorkspaceConfig"/>
    <many-to-many ref="com.axelor.apps.base.db.Partner" name="partnerSet"
      title="Partners"/>

  </entity>

</domain-models>
