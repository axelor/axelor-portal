<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  https://axelor.com/xml/ns/domain-models/domain-models_7.1.xsd">

  <module name="portal" package="com.axelor.apps.portal.db"/>

  <entity name="Hyperlink">
    <many-to-one name="logo" ref="com.axelor.meta.db.MetaFile" title="Logo"/>
    <string name="link" title="Link" required="true"/>
    <many-to-one name="portalAppConfig" ref="com.axelor.apps.portal.db.PortalAppConfig"/>
  </entity>

</domain-models>
