<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="portal" package="com.axelor.apps.portal.db"/>

  <entity name="Registration">

    <many-to-one name="event" ref="com.axelor.apps.portal.db.PortalEvent"/>
    <many-to-many ref="com.axelor.apps.portal.db.PortalParticipant"
      name="participantSet" title="Event participants"/>

  </entity>

</domain-models>
