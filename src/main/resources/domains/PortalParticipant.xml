<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="portal" package="com.axelor.apps.portal.db"/>

  <entity name="PortalParticipant">

    <string name="titleSelect" title="Title" selection="partner.title.type.select"/>
    <string name="name"/>
    <string name="surname"/>
    <string name="emailAddress"/>
    <string name="phone"/>
    <string name="company"/>
    <string name="contactAttrs" json="true" title="Contact infos"/>
    <integer name="sequence"/>
    <boolean name="isAttended" title="Attended"/>
    <many-to-one name="contact" ref="com.axelor.apps.base.db.Partner"/>
    <many-to-one ref="com.axelor.apps.portal.db.Registration" name="registration"/>
    <many-to-many ref="com.axelor.apps.portal.db.PortalEventFacility"
      name="subscriptionSet" title="Subscriptions"/>

  </entity>

</domain-models>
