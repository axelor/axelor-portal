<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="portal" package="com.axelor.apps.portal.db"/>

  <entity name="PortalPaymentConfig">

    <string name="name"/>
    <string name="typeSelect" selection="payment.config.type.select"/>
    <many-to-one ref="com.axelor.apps.portal.db.WebsiteConfig" name="websiteConfig"/>

    <unique-constraint columns="websiteConfig,typeSelect"/>

  </entity>

</domain-models>
