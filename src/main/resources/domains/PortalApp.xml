<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="portal" package="com.axelor.apps.portal.db"/>

  <entity name="PortalApp">

    <string name="name"/>
    <string name="description" large="true"/>
    <many-to-many name="dependnecySet" ref="com.axelor.apps.portal.db.PortalApp"
      title="Dependencies"/>
    <string name="installed" selection="app.install.select"/>

  </entity>

</domain-models>
